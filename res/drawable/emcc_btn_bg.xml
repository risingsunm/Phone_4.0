<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Non focused states -->
    <item android:state_focused="false" android:state_selected="false"
        android:state_pressed="false" android:drawable="@drawable/emcc_btn_normal" />
    <item android:state_focused="false" android:state_selected="true"
        android:state_pressed="false" android:drawable="@drawable/emcc_btn_press" />

    <!-- Focused states -->
    <item android:state_focused="true" android:state_selected="true"
        android:state_pressed="false" android:drawable="@drawable/emcc_btn_press" />

    <!-- Pressed -->
    <item android:state_pressed="true" android:drawable="@drawable/emcc_btn_press" />

</selector>
<!-- End: Modified by zxiaona for bug for emergency 2012/07/11  -->